public class MyClass {

    public static void findFactors (int n) {
        for(int i = 1;i <= Math.sqrt(n);i++) {
            if (n % i == 0) {
                System.out.print(i + " ");
            }
        }
        for(int i = (int)Math.sqrt(n) - 1;i >= 1;i--) {
            if (n % i == 0) {
                System.out.print(n/i + " ");
            }
        }
    }

    public static void main(String args[]) {
      findFactors(100);
    }
}